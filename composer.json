{
    "name": "shonm/resque-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle for Resque via php-resque",
    "keywords": ["resque", "redis", "woker", "queue", "php-resque"],
    "homepage": "https://github.com/ShonM/ResqueBundle",
    "license": "dbad",
    "authors": [
        {
            "name": "Shon M",
            "email": "him@shonm.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1,<3.0-dev",
        "chrisboulton/php-resque": "*"
    },
    "require-dev": {
        "symfony/yaml": "*",
        "symfony/class-loader": "*",
        "symfony/process": "*"
    },
    "autoload": {
        "psr-0": { "ShonM\\ResqueBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "ShonM/ResqueBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
