{
    "name": "shonm/resque-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle for Resque via php-resque",
    "keywords": ["resque", "redis", "woker", "queue", "php-resque"],
    "homepage": "https://github.com/ShonM/ResqueBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Shon M",
            "email": "him@shonm.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "shonm/php-resque",
                "version": "dev-fastcgi",
                "source": {
                    "url": "https://github.com/ShonM/php-resque.git",
                    "type": "git",
                    "reference": "fastcgi"
                },
                "autoload": {
                    "psr-0": {"Resque": "lib/"}
                }
            }
        }
    ],
    "require-dev": {
        "symfony/symfony": ">=2.1,<3.0-dev",
        "shonm/php-resque": "dev-fastcgi",
        "colinmollenhour/credis": "~1.2",
        "mtdowling/cron-expression": "dev-master",
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-0": { "ShonM\\ResqueBundle": "" }
    },
    "target-dir": "ShonM/ResqueBundle",
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
